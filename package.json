{
  "name": "n-style",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "webpack": "webpack",
    "watch:webpack": "webpack --watch",
    "compile:ejs2html": "ejs-cli -b src/ejs/ \"/**/!(_)*.ejs\" -o dist/",
    "start:server": "browser-sync start -s dist/ -w src/*.html src/assets/css/*.css",
    "beautify": "js-beautify -r dist/*.html dist/**/*.html dist/**/**/*.html -s 2 -p false",
    "compile:css2cssprefix": "postcss dist/assets/css/style.css -u autoprefixer -o dist/assets/css/style.css",
    "compile:scss2css": "node-sass src/assets/scss/style.scss --importer node_modules/node-sass-globbing/index.js dist/assets/css/style.css --output-style expanded --source-map dist/assets/css/",
    "compile:scss2cssprefix": "run-s compile:scss2css compile:css2cssprefix",
    "watch:scss2cssprefix": "chokidar \"src/assets/scss/\" --command \"npm run compile:scss2cssprefix\" --initial",
    "watch:ejs2html": "chokidar \"src/ejs/**/*\" -c \"npm run compile:ejs2html\" --initial",
    "watch:img": "chokidar \"src/assets/img/**/*\" --command \"node imagemin.mjs\" --initial",
    "start": "run-p watch:* webpack start:server",
    "clean": "rimraf dist",
    "min:css": "cleancss -o dist/css/assets/style.css dist/css/assets/style.css",
    "min:js": "uglifyjs dist/js/assets/main.js -o dist/js/assets/main.js",
    "delete:map": "rimraf dist/**/*.map",
    "prepare:all": "run-p min:js min:css delete:map",
    "delete:dist": "rimraf dist"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.18.2",
    "@babel/preset-env": "^7.18.2",
    "autoprefixer": "^10.4.7",
    "babel-loader": "^8.2.5",
    "browser-sync": "^2.27.10",
    "chokidar-cli": "^3.0.0",
    "clean-css-cli": "^5.6.0",
    "cpx": "^1.5.0",
    "css-loader": "^6.7.1",
    "ejs-cli": "^2.2.3",
    "imagemin-cli": "^7.0.0",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-keep-folder": "^5.3.2",
    "imagemin-mozjpeg": "^10.0.0",
    "imagemin-pngquant": "^9.0.2",
    "imagemin-svgo": "^10.0.1",
    "mkdirp": "^1.0.4",
    "node-sass": "^7.0.1",
    "node-sass-globbing": "^0.0.23",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.13",
    "postcss-cli": "^9.1.0",
    "sass": "^1.51.0",
    "style-loader": "^3.3.1",
    "stylus": "^0.57.0",
    "uglify-es": "^3.3.9",
    "webpack": "^5.72.1",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {
    "@types/animejs": "^3.1.4",
    "animejs": "^3.2.1",
    "imagemin": "^7.0.1",
    "jquery": "^3.6.0",
    "js-beautify": "^1.14.4",
    "microcms-js-sdk": "^2.3.2",
    "onchange": "^7.1.0",
    "scroll-hint": "^1.2.5",
    "scroll-out": "^2.2.12",
    "skrollr": "^0.6.26",
    "slick-carousel": "^1.8.1",
    "smoothscroll-polyfill": "^0.4.4",
    "splitting": "^1.0.6",
    "watch": "^1.0.2"
  }
}
